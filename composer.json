{
    "name": "jalle19/laravel-deferred-event-dispatcher",
    "description": "A deferring event dispatcher for the Laravel and Lumen frameworks",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Stenvall",
            "email": "sam.stenvall@digia.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/contracts": "^5.4|^6.0|^7.0|^8.0|^9.0",
        "illuminate/events": "^5.4|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Jalle19\\Laravel\\Events\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jalle19\\Laravel\\Events\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
